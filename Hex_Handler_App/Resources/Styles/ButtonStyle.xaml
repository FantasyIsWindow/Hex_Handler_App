    <Style TargetType="{x:Type Button}" x:Key="GeneralButtonStyle">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Width" Value="25"/>
        <Setter Property="Height" Value="25"/>        
    </Style>

    <Style TargetType="{x:Type Button}" x:Key="SaveButtonStyle" BasedOn="{StaticResource GeneralButtonStyle}">
        <Setter Property="Content" Value="{StaticResource SaveIcon}"/>
    </Style>

    <Style TargetType="{x:Type Button}" x:Key="DuplicatesDelButtonStyle" BasedOn="{StaticResource GeneralButtonStyle}">
        <Setter Property="Padding" Value="1, 1, 0, 0"/>
        <Setter Property="Content" Value="{StaticResource DuplicatesDelIcon}"/>
    </Style>

    <Style TargetType="{x:Type Button}" x:Key="SearchButtonStyle" BasedOn="{StaticResource GeneralButtonStyle}">
        <Setter Property="Margin" Value="0, 0, 1, 0"/>
        <Setter Property="Content" Value="{StaticResource SearchIcon}"/>
    </Style>

    <Style TargetType="{x:Type Button}" x:Key="OpenFolderButtonStyle" BasedOn="{StaticResource GeneralButtonStyle}">
        <Setter Property="Margin" Value="0, 0, 1, 0"/>
        <Setter Property="Content" Value="{StaticResource OpenFolderIcon}"/>
    </Style>
    
</ResourceDictionary>
