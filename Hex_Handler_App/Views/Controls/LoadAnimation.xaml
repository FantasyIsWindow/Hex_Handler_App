                <EasingColorKeyFrame KeyTime="0:0:0.7" Value="#FF009696"/>
            </ColorAnimationUsingKeyFrames>
        </Storyboard>
    </UserControl.Resources>
    <Grid>
        <Ellipse x:Name="ellipse7" HorizontalAlignment="Left" Height="25" Width="25" Margin="75,0,0,0"   VerticalAlignment="Top" Fill="#7F009696"/>
        <Ellipse x:Name="ellipse6" HorizontalAlignment="Left" Height="25" Width="25" Margin="110,35,0,0" VerticalAlignment="Top" Fill="#7F009696"/>
        <Ellipse x:Name="ellipse5" HorizontalAlignment="Left" Height="25" Width="25" Margin="110,80,0,0" VerticalAlignment="Top" Fill="#7F009696"/>
        <Ellipse x:Name="ellipse4" HorizontalAlignment="Left" Height="25" Width="25" Margin="75,115,0,0" VerticalAlignment="Top" Fill="#7F009696"/>
        <Ellipse x:Name="ellipse3" HorizontalAlignment="Left" Height="25" Width="25" Margin="30,115,0,0" VerticalAlignment="Top" Fill="#7F009696"/>
        <Ellipse x:Name="ellipse2" HorizontalAlignment="Left" Height="25" Width="25" Margin="0,80,0,0"   VerticalAlignment="Top" Fill="#7F009696"/>
        <Ellipse x:Name="ellipse1" HorizontalAlignment="Left" Height="25" Width="25" Margin="0,35,0,0"   VerticalAlignment="Top" Fill="#7F009696"/>
        <Ellipse x:Name="ellipse0" HorizontalAlignment="Left" Height="25" Width="25" Margin="30,0,0,0"   VerticalAlignment="Top" Fill="#7F009696"/>
        <Label Content="Processing..." FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Grid>
</UserControl>
